{
  "name": "@magnal/prettier-config",
  "version": "3.0.2",
  "description": "Prettier config for my projects",
  "author": {
    "name": "Manuel Lang",
    "email": "hello@manuellang.dev",
    "url": "http://manuellang.dev/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MaGnaL/prettier-config.git"
  },
  "homepage": "https://github.com/MaGnaL/prettier-config",
  "main": ".prettierrc.json",
  "scripts": {
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "standard-version": "^9.1.0"
  },
  "peerDependencies": {
    "husky": "^4.3",
    "prettier": "^2.2",
    "pretty-quick": "^3.1"
  }
}
